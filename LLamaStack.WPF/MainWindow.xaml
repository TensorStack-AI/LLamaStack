<Window x:Class="LLamaStack.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:LLamaStack.WPF.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280" Name="UI" FontSize="14" >
    <DockPanel DataContext="{Binding ElementName=UI}">
        <TabControl>
            <TabItem Header="Inference">
                <views:InferenceView />
            </TabItem>
            <TabItem Header="Output Log" >
                <views:LogWindowView LogOutput="{Binding OutputLog}"/>
            </TabItem>
        </TabControl>
    </DockPanel>
 
</Window>
